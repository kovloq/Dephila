<p id="notice"><%= notice %></p>

<h1>Administrator News</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @administrator_news.each do |administrator_news| %>
      <tr>
        <td><%= link_to 'Show', administrator_news %></td>
        <td><%= link_to 'Edit', edit_administrator_news_path(administrator_news) %></td>
        <td><%= link_to 'Destroy', administrator_news, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

