<p id="notice"><%= notice %></p>

<h1>Administrator Contacts</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @administrator_contacts.each do |administrator_contact| %>
      <tr>
        <td><%= link_to 'Show', administrator_contact %></td>
        <td><%= link_to 'Edit', edit_administrator_contact_path(administrator_contact) %></td>
        <td><%= link_to 'Destroy', administrator_contact, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Administrator Contact', new_administrator_contact_path %>
